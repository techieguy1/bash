export GRAPHVIZ_DOT=/usr/local/bin/dot
export JAVA_HOME=/opt/java11
export JRE_HOME=${JAVA_HOME}

# History setup for all machines.
export HISTIGNORE="ls:ll:history:h:&:[bf]g:dig*:ping:pwd:exit"
export HISTTIMEFORMAT='%F %T '
export HISTSIZE=1000
export HISTFILE=${HOME}/.bash_hist

export PATH="${JAVA_HOME}/bin:${JRE_HOME}/bin:/Library/Frameworks/Python.framework/Versions/3.7/bin:${GRAPHVIZ_DOT}/bin:${PATH}"
